{
    "name": "zeppasta",
    "displayName": "ZepPasta",
    "description": "@stcgoal An experimentation using Zeplin to generate Web App CSS and HTML",
    "version": "0.0.1",
    "publisher": "guillaumeisabelle",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.sayHello2"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        },{
            "command": "extension.sayHello2",
            "title": "Hello World 2"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}